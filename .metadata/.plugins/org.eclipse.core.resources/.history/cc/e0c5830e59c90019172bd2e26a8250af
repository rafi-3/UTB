		Scanner scanner = new Scanner(System.in);
		String text1, text2;
		
		System.out.println("First text: ");
		text1 = scanner.nextLine();
		System.out.println("Second text: ");
		text2 = scanner.nextLine();
		
		
		scanner.close();
	}

