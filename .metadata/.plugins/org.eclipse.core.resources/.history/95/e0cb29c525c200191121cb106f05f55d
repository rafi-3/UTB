package week_3;

public class SumAndAverage {
	public static void main(String[] args) {
		int one, two, three, four, five, total;
		one = 1;
		two = 2;
		three = 3;
		four = 4;
		five = 5;
		
		total = one + two + three + four + five;
		
		System.out.println("sum of five variable is " +total);
		
		
	}
}
